<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.douzone.dzfinal.repository.MemberRepository">
	<insert id="insert">
		insert into tb_dzfinal_member (
			id
			, pwd
		) values (
			#{id}
			, #{pwd}
		)
	</insert>
	<select id="findOneById" resultType="Member">
		select * from tb_dzfinal_member where id = #{id}
	</select>
</mapper>